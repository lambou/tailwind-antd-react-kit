{
  "name": "tailwind-antd-react-kit",
  "version": "1.5.4",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "private": false,
  "files": [
    "dist",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/lambou/tailwind-antd-react-kit"
  },
  "homepage": "https://lambou.github.io/tailwind-antd-react-kit",
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "test": "craco test",
    "eject": "craco eject",
    "deploy-storybook": "storybook-to-ghpages",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "clean": "rimraf dist",
    "compile": "npm run clean && tsc && cross-env NODE_ENV=production babel src/components --out-dir dist --copy-files --extensions '.ts,.tsx' --ignore __tests__,spec.js,test.js,stories.js,__snapshots__",
    "prepublish": "npm run compile"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      },
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "react-is": "^17.0.2",
    "twin.macro": "^2.8.2",
    "web-vitals": "^1.1.2"
  },
  "peerDependencies": {
    "@ant-design/icons": ">= 4.7.0",
    "@noreajs/common": "^1.1.4",
    "antd": ">= 4.18.3",
    "axios": ">= 0.24.0",
    "backend-gateway": ">= 0.0.2",
    "clsx": ">= 1.1.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "5.x",
    "styled-components": ">= 5.3.3"
  },
  "peerDependenciesMeta": {
    "@ant-design/icons": {
      "optional": false
    },
    "@noreajs/common": {
      "optional": false
    },
    "antd": {
      "optional": false
    },
    "axios": {
      "optional": false
    },
    "backend-gateway": {
      "optional": false
    },
    "clsx": {
      "optional": false
    },
    "react": {
      "optional": false
    },
    "react-dom": {
      "optional": false
    },
    "react-router-dom": {
      "optional": false
    },
    "styled-components": {
      "optional": false
    }
  },
  "devDependencies": {
    "@ant-design/icons": "^4.7.0",
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.6",
    "@babel/plugin-transform-typescript": "^7.14.6",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.14.5",
    "@craco/craco": "^6.4.3",
    "@noreajs/common": "^1.1.4",
    "@storybook/addon-actions": "^6.3.8",
    "@storybook/addon-essentials": "^6.3.8",
    "@storybook/addon-links": "^6.3.8",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/addon-storyshots": "^6.1.21",
    "@storybook/node-logger": "^6.3.8",
    "@storybook/preset-create-react-app": "^3.2.0",
    "@storybook/react": "^6.3.8",
    "@storybook/storybook-deployer": "^2.8.10",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.9.6",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "@types/react-is": "^17.0.1",
    "@types/react-router-dom": "^5.1.7",
    "@types/styled-components": "^5.1.11",
    "antd": "^4.18.4",
    "autoprefixer": "^9.8.6",
    "axios": "^0.24.0",
    "backend-gateway": "^0.0.2",
    "clsx": "^1.1.1",
    "craco": "^0.0.3",
    "craco-less": "^1.20.0",
    "cross-env": "^7.0.3",
    "postcss": "^7.0.36",
